<!DOCTYPE html>
<html lang="en">

<head>
    <!-- PACE Loader -->
    <%= javascript_include_tag "pace.min.js"%>
    <%= stylesheet_link_tag 'pace.css' %>
    <!-- END PACE Loader -->

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="images/favicon/favicon.ico" type="image/x-icon">
    <link rel="icon" href="images/favicon/favicon.ico" type="image/x-icon">

    <title>Garis Creative Agency Template</title>

    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">

    <!-- Bootstrap core CSS -->
    <%= stylesheet_link_tag 'bootstrap.min.css' %>
    <!-- Font Awesome CSS -->
    <%= stylesheet_link_tag 'font-awesome.css' %>
    <!-- Zocial CSS -->
    <%= stylesheet_link_tag 'zocial.css' %>
    <!-- Nivo Lightbox CSS -->
    <%= stylesheet_link_tag 'nivo-lightbox.css' %>
    <%= stylesheet_link_tag 'default.css' %>
    <!-- STYLE CSS -->
    <%= stylesheet_link_tag 'style.css' %>
    <!-- Color Scheme CSS -->
    <%= stylesheet_link_tag 'blue.css' %>

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->
</head>

<body data-spy="scroll">
    <div id="loader"></div>
    <div id="main-wrapper">
        <div id="sidebar">
            <nav class="navbar navbar-default" role="navigation">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#">
                        Cheer! <i class="fa fa-child"></i>
                    </a>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                        <li class="active"><a class="sscroll" href="#mypage">Mypage</a>
                        </li>
                        <li><%= link_to "Users",users_path %>
                        </li>
                        <li><%= link_to "Post",posts_path %>
                        </li>
                        <li><%= link_to "Information",information_index_path %>
                        </li>
                        <li><%= link_to "Journal",journals_path %>
                        </li>
                
  <li><%= link_to "Menu",menus_path %>
                        </li>
                        <li><%= link_to "Videos" %>
                        </li>
                    </ul>
                </div>
                <!-- /.navbar-collapse -->
            </nav>
          </div>
    <div id="container">
      <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
      <% end %>

      <div class="wrapper">
        <!-- ここのページがここで読み込まれる -->
        <%= yield %>
      </div>


      <%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <!-- Jquery Source -->
    <%= javascript_include_tag "jquery-1.10.2.min.js"%>
    <!-- Nivo Light Box Source -->
    <%= javascript_include_tag "nivo-lightbox.min.js"%>
    <!-- Bootstrap Source -->
    <%= javascript_include_tag "bootstrap.min.js"%>
    <!-- Superslides Source -->
    <%= javascript_include_tag "jquery.superslides.min.js"%>
    <!-- Smoothscroll Source -->
    <%= javascript_include_tag "smoothscroll.js"%>
    <!-- Sudo Slider Source -->
    <%= javascript_include_tag "jquery.sudoslider.min.js"%>
    <!-- Mixitup Source -->
    <%= javascript_include_tag "jquery.mixitup.min.js"%>
    <!-- Back to Top Source -->
    <%= javascript_include_tag "jquery.backtotop.js"%>
    <!-- Backstretch Source -->
    <%= javascript_include_tag "jquery.backstretch.min.js"%>
    <!-- CarouFredSel Source -->
    <%= javascript_include_tag "jquery.carouFredSel-6.2.1-packed.js"%>
    <!-- Mobile Responsive Javascript -->
    <%= javascript_include_tag "jquery.mobileresponsive.js"%>
    <!-- Google Map Source -->
    <%= javascript_include_tag "gmap3.min.js"%>

    <script src="https://maps.googleapis.com/maps/api/js?sensor=true"></script>
    <!-- Main Action Javascript -->
    <%= javascript_include_tag "main.js"%>
</body>
</html>